<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/11.0.1" fx:controller="View_Controller.ModifyPartController">
   <children>
       <AnchorPane layoutX="10.0" layoutY="10.0" prefHeight="609.0" prefWidth="600.0">
         <children>
            <Label layoutX="32.0" layoutY="32.0" prefHeight="17.0" prefWidth="80.0" text="Modify Part" />
             <RadioButton fx:id="inHouseButton" layoutX="213.0" layoutY="32.0" mnemonicParsing="false"
                          onAction="#inHouseTrue" selected="true" text="In-House">
                 <toggleGroup>
                     <ToggleGroup fx:id="modifyToggle"/>
                 </toggleGroup>
             </RadioButton>
             <RadioButton fx:id="Outsourced" layoutX="326.0" layoutY="32.0" mnemonicParsing="false"
                          onAction="#outsourcedTrue" text="Outsourced" toggleGroup="$modifyToggle"/>
            <Label layoutX="88.0" layoutY="119.0" text="ID" />
             <TextField fx:id="partIDBox" layoutX="124.0" layoutY="104.0" onAction="#idHandler" prefHeight="47.0"
                        prefWidth="192.0" promptText="Part ID"/>
            <Label layoutX="71.0" layoutY="179.0" text="Name" />
            <TextField fx:id="Name" layoutX="124.0" layoutY="164.0" onAction="#nameHandler" prefHeight="47.0" prefWidth="192.0" promptText="Part Name" />
            <Label layoutX="84.0" layoutY="236.0" text="Inv" />
            <TextField fx:id="Inv" layoutX="124.0" layoutY="221.0" onAction="#invHandler" prefHeight="47.0" prefWidth="192.0" promptText="Inv" />
            <Label layoutX="48.0" layoutY="293.0" prefHeight="17.0" prefWidth="55.0" text="Price/Cost" />
            <TextField fx:id="PriceCost" layoutX="124.0" layoutY="278.0" onAction="#priceCostHandler" prefHeight="47.0" prefWidth="192.0" promptText="Price/Cost" />
            <Label layoutX="81.0" layoutY="358.0" text="Max" />
            <TextField fx:id="Max" layoutX="124.0" layoutY="343.0" onAction="#maxHandler" prefHeight="47.0" prefWidth="192.0" promptText="Max" />
            <Label layoutX="337.0" layoutY="358.0" text="Min" />
            <TextField fx:id="Min" layoutX="390.0" layoutY="343.0" onAction="#minHandler" prefHeight="47.0" prefWidth="192.0" promptText="Min" />
            <Label layoutX="18.0" layoutY="420.0" text="Company Name" />
            <TextField fx:id="CompName" layoutX="124.0" layoutY="405.0" onAction="#companyNameHandler" prefHeight="47.0" prefWidth="192.0" promptText="Comp Name" />
             <Button fx:id="SaveButton" layoutX="231.0" layoutY="554.0" mnemonicParsing="false"
                     onAction="#saveButtonHandler" text="Save"/>
             <Button fx:id="CancelButton" layoutX="326.0" layoutY="554.0" mnemonicParsing="false"
                     onAction="#cancelButtonHandler" text="Cancel"/>
             <TextField fx:id="MachineID" layoutX="124.0" layoutY="472.0" onAction="#machineIDHandler" prefHeight="47.0"
                        prefWidth="192.0" promptText="Machine ID"/>
             <Label fx:id="machineIDLabel" layoutX="44.0" layoutY="487.0" text="Machine ID"/>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
